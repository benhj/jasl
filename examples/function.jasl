// test a function called foo.
// Note, takes an empty list of parameters.
block foo (a, b, c, d, e, f, g) {
    pr "a: "; prn a;
    pr "b: "; prn b;
    pr "c: "; prn c;
    pr "d: "; prn d;
    pr "e: "; prn e;
    pr "f: "; prn f;
    pr "g: "; prn g;
    prn "Now in function";

}

// entry point
start {
    prn "Going in to function..";
    int 1 -> a;
    real 5.5 -> b;
    string "goodbye" -> c;
    list [another [nested] list] -> d;
    call foo (a, b, 10, "hello", c, [a list], d);
}


